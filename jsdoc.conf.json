{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["script", "docs/API.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "destination": "./docs/api",
        "encoding": "utf8",
        "recurse": true,
        "hierarchy": true,
        "template": "./node_modules/docdash"
    },
    "docdash": {
        "search": true,
        "typedefs": true,
        "collapse": true
    }
}